intents:
{% for intent in intents %}
  - {{intent["name"]}}
{% endfor %}
{% for event in events %}
  - {{event["name"]}}
{% endfor %}

{% if entities %}
entities:
{% for entity in entities %}
  - {{entity["name"]}}
{% endfor %}
{% for entity in pretrained_entities %}
  - {{entity}}
{% endfor %}
{% endif %}

{% if slots %}
slots:
{% for slot in slots %}
  {{slot.name}}:
    type: {{slot.type}}
    influence_conversation: false
    mappings:
    - type: custom
{% endfor %}
{% endif %}

{% if forms %}
forms:
{% for form in forms %}
  {{form.name}}:
    required_slots:
    {% for slot in form.slots %}
    - {{slot}}
    {% endfor %}
{% endfor %}
{% endif %}

# slots from forms

{% if actions %}
actions:
{% for action in actions %}
  - {{action["name"]}}
{% endfor %}
{% endif %}

{% if responses %}
responses:
  {% for response in responses %}
  {{response.name}}:
  - text: "{{response.text}}"
  {% endfor %}
{% endif %}

session_config:
  session_expiration_time: 5
  carry_over_slots_to_new_session: false

version: '3.1'
