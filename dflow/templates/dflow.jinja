
entities
{%- for entity in entities %}
    Entity {{ entity.name }}
    {%- for sub_entity in entity.sub_entities %}
        {{ sub_entity }}{{ "," if not loop.last else "" }}\n
    {%- endfor %}
    end
{%- endfor %}
end

synonyms
{%- for synonym in synonyms %}
  Synonym {{ synonym.name }}
  {%- for sub_synonym in synonym.sub_synonyms %}
    {{ sub_synonym }}{{ "," if not loop.last else "" }}\n
  {%- endfor %}
  end
{%- endfor %}
end

eservices
    EServiceHTTP {{ service_name }}
        verb: {{ verb }}
        host: '{{ host }}'
        {%- if port %}
        port: {{ port }}
        {%- endif %}
        {%- if path %}
        path: '{{ path }}'
        {%- endif %}
    end